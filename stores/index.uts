import {reactive} from 'vue'
/**
 * 定义state类型约束
 */
export type State = {
	hasLogin: boolean
}

/**
 * 定义state
 */
export const state = reactive({
	hasLogin: false
} as State)

/**
 * 定义actions
 */
export const setData = (data: UTSJSONObject) => {
	data.toMap().forEach((v, k) => {
		state[k] = v
	})
	uni.setStorageSync('state', state)
}

