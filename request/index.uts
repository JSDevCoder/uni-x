import UtxRequest from './request.uts'

const reqIns = new UtxRequest({
	baseURL: 'https://www.fastmock.site/mock/f3db82b0eeba8d6ec09d1947627bb194/uni-x',
	header: {
		'Content-Type': 'application/json'
	}
})

reqIns.beforeInterceptors((config : UTSJSONObject) : UTSJSONObject => {
	const data = config.get('data') as UTSJSONObject | null
	data?.set('param', 'ttt')
	config.set('data', data)
	return config
})

export default reqIns